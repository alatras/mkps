{
  "name": "nestjs-prototype",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "copy": "cp ./config/nftProperties/eos/nftProperties.json ./src/utils/nftProperties/tenant.json",
    "prebuild": "rimraf build && npm run copy",
    "predev": "npm run copy",
    "pretest": "npm run copy",
    "build": "nest build",
    "pretty": "prettier --config ./.prettierrc.json --check '**/*.{ts,json}'",
    "pretty:fix": "prettier --config ./.prettierrc.json --write '**/*.{ts,json}'",
    "start": "nest start",
    "dev": "AWS_PROFILE=eosDev AWS_SDK_LOAD_CONFIG=1 nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node build/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --cache",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_ENV=test.unit jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "dev:hosted": "ACTIVE_SERVICES=NFT_SERVICE,AVN_SERVICE,LISTING_SERVICE concurrently \"npm run ssh:mongo\" \"npm run ssh:redis\" \"npm run dev\" --kill-others",
    "ssh:mongo": "ssh -i ~/.ssh/id_rsa -L 27017:10.150.4.35:27017 ubuntu@3.143.142.127 -N",
    "ssh:redis": "ssh -i ~/.ssh/id_rsa -L 6379:10.150.4.219:6379 ubuntu@3.143.142.127 -N",
    "migrate:create": "sh -c 'ts-node node_modules/.bin/migrate-mongo create $0 -f migrate-mongo-config.ts' ",
    "migrate:status": "ts-node node_modules/.bin/migrate-mongo status -f migrate-mongo-config.ts",
    "migrate:undo-last": "ts-node node_modules/.bin/migrate-mongo down -f migrate-mongo-config.ts && npm run migrate:status",
    "migrate": "ts-node node_modules/.bin/migrate-mongo up -f migrate-mongo-config.ts"
  },
  "dependencies": {
    "@nestjs/axios": "^2.0.0",
